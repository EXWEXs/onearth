<?xml version="1.0" encoding="UTF-8"?>
<EnvironmentConfiguration>
    <GetCapabilitiesLocation service="wmts">/etc/onearth/config/test/test_data/wmts_endpoint/</GetCapabilitiesLocation>
    <GetCapabilitiesLocation service="twms">/etc/onearth/config/test/test_data/twms_endpoint/.lib/</GetCapabilitiesLocation>
    <GetTileServiceLocation>/etc/onearth/config/test/test_data/twms_endpoint/.lib/</GetTileServiceLocation>
    <CacheLocation service="wmts" basename="cache_all_wmts">/etc/onearth/config/test/test_data/</CacheLocation>
    <CacheLocation service="twms" basename="cache_all_twms">/etc/onearth/config/test/test_data/</CacheLocation>
    <StagingLocation service="wmts">/usr/share/onearth/layer_config/wmts/test/</StagingLocation>
    <StagingLocation service="twms">/usr/share/onearth/layer_config/twms/test/</StagingLocation>
    <ServiceURL service="wmts">http://localhost/onearth/test/wmts/</ServiceURL>
    <ServiceURL service="twms">http://localhost/onearth/test/twms</ServiceURL>
    <LegendLocation>/usr/share/onearth/demo/legends/</LegendLocation>
    <LegendURL>http://localhost/legends/</LegendURL>
    <ColorMapLocation>/usr/share/onearth/demo/colormaps/</ColorMapLocation>
    <ColorMapURL>http://localhost/colormaps/</ColorMapURL>
    <Mapfile>/usr/share/onearth/demo/mapserver/epsg4326.map</Mapfile>
    <MapfileConfigLocation basename="EPSG4326">/etc/onearth/config/mapserver/</MapfileConfigLocation>
</EnvironmentConfiguration>
